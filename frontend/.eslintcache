\" component={Itineraries}/>\r\n        <Route path=\"/signUp\" component={SignUp}/>\r\n        <Route path=\"/login\" component={SignIn}/>\r\n        <Redirect to=\"/\"/>\r\n      </Switch>\r\n    </>\r\n  }else if(localStorage.getItem('token')){\r\n    props.signInFromLS(localStorage.getItem('token'))\r\n    .then(response=> {\r\n     response && setReload(!reload)\r\n    })\r\n  }else {\r\n   var routes=\r\n    <>\r\n      <Switch>\r\n        <Route exact path=\"/\" component={Home}/>\r\n        <Route path=\"/cities\" component={Cities}/>\r\n        <Route path=\"/itineraries/:id\" component={Itineraries}/>\r\n        <Route path=\"/signUp\" component={SignUp}/>\r\n        <Route path=\"/login\" component={SignIn}/>\r\n        <Redirect to=\"/\"/>\r\n      </Switch>\r\n    </>\r\n  }\r\n  return (\r\n   <> \r\n   <Router>\r\n      <Header/>\r\n      <Switch>\r\n      {routes}\r\n      </Switch>\r\n       <Footer/>\r\n   </Router>\r\n   </> \r\n  );\r\n}\r\nconst mapStateToProps = state =>{\r\n  return {\r\n    loggedUser: state.authR.loggedUser\r\n  }\r\n}\r\n\r\nconst mapDispatchToProps = {\r\n  signInFromLS: authActions.signInFromLS,\r\n  /* addComment: commentActions.addComment */\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\r\n","C:\\Users\\Mauricio\\Desktop\\Mytinerary\\alcalde-mytinerary\\frontend\\src\\redux\\reducers\\principalReducer.js",[],"C:\\Users\\Mauricio\\Desktop\\Mytinerary\\alcalde-mytinerary\\frontend\\src\\components\\Section.js",[],"C:\\Users\\Mauricio\\Desktop\\Mytinerary\\alcalde-mytinerary\\frontend\\src\\components\\Header.js",["122","123"],"C:\\Users\\Mauricio\\Desktop\\Mytinerary\\alcalde-mytinerary\\frontend\\src\\components\\Cities.js",["124","125"],"import {useState, useEffect} from \"react\"\r\nimport City from \"./City\"\r\nimport {Spinner} from 'reactstrap'\r\nimport {Link} from \"react-router-dom\"\r\nimport fotito from \"../assets/imagenAvion2.jpg\"\r\nimport {connect} from \"react-redux\"\r\nim